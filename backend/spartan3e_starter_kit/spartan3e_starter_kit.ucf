#
# Soldered 50MHz clock.
#
NET "clk" LOC = "C9";

#
# Use button "south" as reset.
#
NET "reset" LOC = "K17" | PULLDOWN ;

#
# UART serial port (RS232 DCE) - connector DB9 female.
#
NET "uart_srx" LOC = "R7";
NET "uart_stx" LOC = "M14" | DRIVE = 8 | SLEW = SLOW ;

